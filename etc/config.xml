<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <dev>
            <js>
                <minify_exclude>
                    /tiny_mce/
                </minify_exclude>
            </js>
            <css>
                <minify_exclude>
                    /bundle.css
                    /tiny_mce/
                </minify_exclude>
            </css>
        </dev>
        <order>
            <order_print>
                <enable>1</enable>
            </order_print>
        </order>
    </default>
</config>
